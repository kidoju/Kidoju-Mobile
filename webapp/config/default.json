{
    "application": {
        "name": "Kidoju.Webapp",
        "scheme": "com.kidoju.webapp",
        "siteName": "Kidoju"
    },
    "assets": {
        "audio": {
            "collections": [
                {
                    "source": "summary"
                }
            ],
            "extensions": [".mp3", ".ogg"],
            "schemes": {
                "cdn": "https://cdn.kidoju.com/"
            }
        },
        "icon": {
            "collections": [
                {
                    "name": "Icons",
                    "transport": {
                        "read": "https://cdn.kidoju.com/images/o_collection/svg/office/index.json"
                    }
                }
            ],
            "extensions": [".svg"],
            "schemes": {
                "cdn": "https://cdn.kidoju.com/"
            }
        },
