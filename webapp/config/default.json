                {
                    "source": "websearch",
                    "params": {
                        "provider": "google",
                        "searchType": "image",
                        "pageSize": 10
                    }
                }
            ],
            "extensions": [".gif", ".jpg", ".png", ".svg"],
            "schemes": {
                "cdn": "https://cdn.kidoju.com/"
            }
        },
        "video": {
            "collections": [
                {
                    "source": "summary"
                }
            ],
            "extensions": [".mp4", ".ogv", ".wbem"],
            "schemes": {
                "cdn": "https://cdn.kidoju.com/"
            }
        }
    },
    "awss3": {
        "region": "us-east-1",
        "config": {
            "bucket": "kidoju.config",
            "key": "kidoju.webapp/production.json"
        }
    },
    "express": {
        "port"  : 3000
    },
    "google": {
        "tagManager": {
            "web": "GTM-PH3KJJ",
            "amp": "GTM-K2QF3MM"
        }
    },
    "head" : "<meta name=\"p:domain_verify\" content=\"00a8ab216c21a6cd727d5d50881ceb1e\"/>",
    "images": ["https://cdn.kidoju.com/kidoju/home1.jpg", "https://cdn.kidoju.com/kidoju/home2.jpg", "https://cdn.kidoju.com/kidoju/home3.jpg"],
    "locales" : [ "en", "fr" ],
    "redirect": {
        "http": {
            "match": "kidoju.com$",
            "forward": "https://www.kidoju.com"
        }
    },
    "slack": {
        "webhook": "https://hooks.slack.com/services/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"
    },
    "twitter": {
        "account": "kidoju"
    },
    "uris" : {
        "amp": {
            "home": "/amp",
            "finder": "/amp/%s",
            "summary": "/amp/%s/s/%s",
            "user": "/amp/%s/u/%s"
        },
        "cdn": {
            "root":  "https://cdn.kidoju.com",
            "icons": "/images/o_collection/svg/office/%s.svg",
            "images": "/kidoju/%s"
        },
        "help": {
            "root":  "https://help.kidoju.com"
        },
        "mobile": {
            "icons": "./img/icons/%s.svg",
            "pictures": "%susers/%s"
        },
        "rapi": {
            "logger": "/api/logger",
            "v1": {
                "categories": "/api/v1/languages/%s/categories",
                "activities": "/api/v1/%s/summaries/%s/activities",
                "files": "/api/v1/%s/summaries/%s/files",
                "summary": "/api/v1/%s/summaries/%s",
                "summaries": "/api/v1/%s/summaries",
                "user": "/api/v1/users/%s"
            },
            "web": {
                "search": "/api/web/search/%s"
            }
        },
        "webapp": {
            "editor": "/%s/e/%s/%s",
            "error": "/%s/error",
            "finder": "/%s",
            "home": "/",
            "locale": "/%s",
            "logger": "/logger",
            "ping": "/ping",
            "player": "/%s/x/%s/%s/%s",
            "proxy": "/proxy",
            "public": "/%s",
            "rss": "/%s/rss/%s",
            "sitemap": "/%s/sitemap/%s",
            "summary": "/%s/s/%s",
            "support": "/support/%s",
            "user": "/%s/u/%s",
            "workerlib": "workerlib.bundle.js"
        }
    }
}
